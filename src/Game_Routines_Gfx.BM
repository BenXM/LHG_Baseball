'------------------------------
'   PBP Subroutine
'------------------------------
' ...explanation...

' Has been updated from original
' source to determine if we're
' printing on GFX or text screen
Sub PBP (F$)
    If displayPause > 0 Then

        Call SPA(F$, 44)
        F$ = Left$(F$, 44)
        PB% = PB% + 1

        ' Put PBP text in a slot for line PB%
        If PB% < 9 Then G$(PB%) = F$

        ' If we're past line 8, move all slots down
        ' to make room
        If PB% > 8 Then
            For I9 = 1 To 7
                G$(I9) = G$(I9 + 1)
            Next
            G$(8) = F$
        End If

        'Print all 8 lines from scratch
        For I9 = 1 To 8
            If usingGfx = 1 Then
                PrintOnGfx 20, I9 + 2, G$(I9), imageScreen&
            Else
                Locate 14 + I9, 1
                Print G$(I9)
            End If
        Next

        Call DELAY
    End If
End Sub


