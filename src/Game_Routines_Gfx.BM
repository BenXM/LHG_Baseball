'------------------------------
'   DISPRUN_G Subroutine
'------------------------------
' ...explanation...
Sub DISPRUN_G (I2, p, X)
    Select Case I2
        Case 1: _PutImage (37 * FontColAdj%, 304)-Step(10 * FontColAdj%, 32), backupScreen&, imageScreen&, (37 * FontColAdj%, 304)-Step(10 * FontColAdj%, 32)
        Case 2: _PutImage (15 * FontColAdj%, 336)-Step(10 * FontColAdj%, 32), backupScreen&, imageScreen&, (15 * FontColAdj%, 336)-Step(10 * FontColAdj%, 32)
        Case 3: _PutImage (56 * FontColAdj%, 336)-Step(10 * FontColAdj%, 32), backupScreen&, imageScreen&, (56 * FontColAdj%, 336)-Step(10 * FontColAdj%, 32)
    End Select

    J = B0%(I2, 0): If J = 0 Then 363
    Y = 22
    If I2 = 2 Then Y = 20

    'Printing white on background
    _PrintMode _KeepBackground , imageScreen&
    Color 4294967295

    If B7%(p, J) = 1 And P%(p, P1%(p), 11) <> 999 Then
        F$ = P$(p, P1%(p))
        Call SPA(F$, 10)

        PrintOnGfx X, Y - 1, F$, imageScreen&

        PrintOnGfx X, Y, "1/  0", imageScreen&

        GoTo 363
    End If

    If B7%(p, J) = 1 Then
        F$ = P$(p, P1%(p))
        Call SPA(F$, 10)

        PrintOnGfx X, Y - 1, F$, imageScreen&
        F$ = FUsing$(Str$(P%(p, P1%(p), 31)), "#", 1) + "/" + FUsing$(Str$(P%(p, P1%(p), 29)), "###", 1)

        PrintOnGfx X, Y, F$, imageScreen&
        GoTo 363
    End If

    F$ = B$(p, B3%(p, J))
    Call SPA(F$, 10)

    PrintOnGfx X, Y - 1, F$, imageScreen&
    F$ = FUsing$(Str$(B%(p, B3%(p, J), 16)), "#", 1) + "/" + FUsing$(Str$(B%(p, B3%(p, J), 13)), "###", 1)

    PrintOnGfx X, Y, F$, imageScreen&

    'Printing white on black
    _PrintMode _FillBackground , imageScreen&

    363 'DONE
End Sub


'------------------------------
